<%= render('layouts/errors', :object => @project) %>

<%= form_for [@skill, @project] do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :features %>
  <%= f.text_field :features %>
  <p><%= f.submit %></p>
<% end %>
